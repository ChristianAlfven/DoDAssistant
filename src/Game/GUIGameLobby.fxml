<AnchorPane prefHeight="384.0" prefWidth="830.0" style="-fx-background-color: #baa88b;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Game.GameLobby">
