            <Lighting diffuseConstant="1.26">
               <bumpInput>
                  <Shadow color="#ffc57d" height="1.0" radius="0.0" width="0.0" />
               </bumpInput>
               <light>
                  <Light.Distant />
               </light>
            </Lighting>
         </effect>
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Button>
      <ListView fx:id="idCharacterTable" layoutX="622.0" layoutY="23.0" prefHeight="228.0" prefWidth="200.0">
         <effect>
            <Lighting>
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant />
               </light>
            </Lighting>
         </effect>
      </ListView>
      <Label layoutX="651.0" layoutY="6.0" prefHeight="17.0" prefWidth="64.0" text="Characters" />
      <Label layoutX="622.0" layoutY="6.0" text="HP" />
