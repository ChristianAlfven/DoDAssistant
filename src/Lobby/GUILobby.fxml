<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="404.0" prefWidth="542.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Lobby.ControllerLobby">
   <children>
      <ImageView fitHeight="455.0" fitWidth="559.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Resources/parchment.png" />
         </image>
      </ImageView>
      <Button fx:id="idNewGame" alignment="CENTER" layoutX="219.0" layoutY="154.0" mnemonicParsing="false" onAction="#buttonNewGame" prefHeight="40.0" prefWidth="121.0" text="New Game">
         <font>
            <Font name="System Bold Italic" size="16.0" />
         </font>
         <effect>
            <Lighting>
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant color="#ffbf80" />
               </light>
            </Lighting>
         </effect>
      </Button>
      <Button fx:id="idLoadGame" alignment="CENTER" layoutX="219.0" layoutY="210.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="121.0" text="Load Game">
         <effect>
            <Lighting>
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant color="#ffbf80" />
               </light>
            </Lighting>
         </effect>
         <font>
            <Font name="System Bold Italic" size="16.0" />
         </font>
      </Button>
      <Button fx:id="idExit" alignment="CENTER" layoutX="219.0" layoutY="267.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="121.0" text="Exit">
         <effect>
            <Lighting>
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant color="#ffbf80" />
               </light>
            </Lighting>
         </effect>
         <font>
            <Font name="System Bold Italic" size="16.0" />
         </font>
      </Button>
      <Label alignment="CENTER" layoutX="129.0" layoutY="46.0" prefHeight="66.0" prefWidth="326.0" text="WELCOME HELLO">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
